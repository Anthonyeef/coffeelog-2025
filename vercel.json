{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/coffee-2025/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/coffee-2025/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/coffee-2025/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/coffee-2025/(.*)",
      "destination": "/coffee-2025/index.html"
    },
    {
      "source": "/coffee-2025",
      "destination": "/coffee-2025/index.html"
    }
  ]
}
